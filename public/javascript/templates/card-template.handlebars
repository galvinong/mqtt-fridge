
    {{#each cards}}
        <div class="card text-center col-md-4 col-md-offset-4" id={{title}} style="max-width: 275px;">
            <div class="card-block">
                <h4 class="card-title">{{title}}</h4>
                <h6 class="card-subtitle">Location:
                              {{location}}</h6>
            </div>
            <div class="card-block">
                <h6 class="saving loading card-title text-muted">
                    Waiting<span>.</span><span>.</span><span>.</span>
                </h6>
                <span class="ready-value">
                    <span class="card-title" id="{{returndata}}"></span>
                    <span class="card-title">{{unitvalue}}</span>
                </span>
            </div>
            <div class="card-block">
                <p class="card-text">
                    {{cardtext}}
                </p>
                <button class="btn col-md-12" id="{{buttonValue}}">
                    <i class="fa fa-trash" aria-hidden="true"></i>
                </button>
                <script>
                $('#{{buttonValue}}').click(function() {
                    $('#{{title}}').remove()
                })
                </script>
            </div>
        </div>
    {{/each}}
